{
  "name": "dynogen",
  "version": "0.0.1",
  "description": "Tool for generating a type safe DynamoDB client based on JSON Schema.",
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "MIT",
  "main": "./dist/src/",
  "module": "./dist/es6/src/",
  "jsnext:main": "./dist/esnext/src/",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "compile-src-cjs": "tsc --declaration --declarationDir ./dist -p tsconfig-src-cjs.json",
    "compile-src-es6": "tsc -p tsconfig-src-es6.json",
    "compile-src-esnext": "tsc -p tsconfig-src-esnext.json",
    "precompile-src": "rm -rf ./dist",
    "compile-src": "yarn compile-src-cjs && yarn compile-src-es6 && yarn compile-src-esnext",
    "lint": "tslint --format codeFrame --project tsconfig.json 'src/**/*.ts' 'test/**/*.ts'",
    "pretest": "yarn lint",
    "test": "jest --coverage",
    "prebuild": "yarn test",
    "build": "yarn compile-src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeomic/dynogen.git"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "peerDependencies": {
    "ajv": "^6.6.0",
    "aws-sdk": "^2.400.0"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.12.2",
    "@types/rimraf": "^2.0.2",
    "@types/uuid": "^3.4.4",
    "ajv": "^6.10.0",
    "aws-sdk": "^2.437.0",
    "jest": "^24.5.0",
    "json-schema": "^0.2.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-semistandard": "^7.0.0",
    "typescript": "^3.4.1",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "json-schema-to-typescript": "^6.1.3",
    "lodash": "^4.17.11"
  }
}
